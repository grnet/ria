<%- include('../partials/head.ejs') %>
  <div class="">
    <!--<input type="button" name="back" class="back" value="Πίσω"/> <br/>--> <a href="#" class="govgr-back-link"
      id="back" name="back"> <svg class="govgr-caret--left" viewBox="0 0 24 24" aria-hidden="true">
        <path d="M18,22V2L6,12L18,22z" />
      </svg>
      Επιστροφή στην αρχική</a> <br />
    <div class="govgr-layout-wrapper govgr-layout-wrapper__full-height">
      <div class="govgr-width-container">
        <div class="govgr-main-wrapper">
          <div class="govgr-grid-column-two-thirds">
            <table id="data" class="govgr-table--zebra govgr-table--dense govgr-table--dark govgr-table">
              <thead class="govgr-table__head">
                <tr class="govgr-table__row">
                  <th class="govgr-table__header" scope="col">Κατηγορία</th>
                  <th class="govgr-table__header" scope="col">Εκθέσεις</th>
                </tr>
              </thead>
              <tbody class="govgr-table__body">
                <tr class="govgr-table__row">
                  <td class="govgr-table__cell">Προσχέδιο νόμου</td>
                  <td class="govgr-table__cell">
                    <%= draftLaws %>
                  </td>
                </tr>
                <tr class="govgr-table__row">
                  <td class="govgr-table__cell">Σχέδιο νόμου</td>
                  <td class="govgr-table__cell">
                    <%= plans %>
                  </td>
                </tr>
                <tr class="govgr-table__row">
                  <td class="govgr-table__cell">Κατευθυντήριες γραμμές</td>
                  <td class="govgr-table__cell">
                    <%= guidelines %>
                  </td>
                </tr>
                <tr class="govgr-table__row">
                  <td class="govgr-table__cell">Επείγοντα και κατεπείγοντα νομοσχέδια</td>
                  <td class="govgr-table__cell">
                    <%= urgent %>
                  </td>
                </tr>
                <tr class="govgr-table__row">
                  <td class="govgr-table__cell">Πρόταση νόμου</td>
                  <td class="govgr-table__cell">
                    <%= proposals %>
                  </td>
                </tr>
                <tr class="govgr-table__row">
                  <td class="govgr-table__cell">Κύρωση διεθνών συμβάσεων</td>
                  <td class="govgr-table__cell">
                    <%= conventions %>
                  </td>
                </tr>
                <tr class="govgr-table__row">
                  <td class="govgr-table__cell">Τροπολογίες Υπουργών</td>
                  <td class="govgr-table__cell">
                    <%= ministerAmendments %>
                  </td>
                </tr>
                <tr class="govgr-table__row">
                  <td class="govgr-table__cell">Τροπολογίες Βουλευτών</td>
                  <td class="govgr-table__cell">
                    <%= parliamentariansAmendments%>
                  </td>
                </tr>
              </tbody>
              <tr class="govgr-table__row">
                <td class="govgr-table__cell"><b>Σύνολο</b></td>
                <td class="govgr-table__cell"> <b>
                    <%= entries %>
                  </b> </td>
              </tr>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
  <%- include('../partials/footer.ejs') %>
    <script type="text/javascript" src="/js/nav_scripts.js"></script>
    <script type="text/javascript" src="/js/enums.js"></script>
    <script>
      $("#back").on("click", function (ev) {
        window.location.href = "/user_views/dashboard";
      })

      const userRole = "<%= user.role %>";
      setUserRestrictions(userRole);

      let isAdmin = '<%= user.isAdmin %>';
    </script>
    <script type="text/javascript" src="/js/common_user_scripts.js"></script>
    </body>

    </html>