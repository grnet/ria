<fieldset id="sunarmodioi_foreis" class="table-31 optional">

    <h2>Η. Έκθεση εφαρμογής της ρύθμισης</h2>
    <br>

    <h4><label id="table_31_label" class="table-31-label optional-label required">31.Συναρμοδιότητα Υπουργείων / υπηρεσιών / φορέων</label></h4>
    <br>

    <table class="table" id="field_31_table">
        <thead class="thead">
            
            <tr>
                <th colspan="1">Σχετική διάταξη αξιολογούμενης ρύθμισης<div class="index_sxetiki_diataksi"></div></th>
                <th colspan="1">Συναρμόδια Υπουργεία – Συναρμόδιες υπηρεσίες / φορείς<div class="index_foreis"></div></th>
                <th colspan="1">Αντικείμενο συναρμοδιότητας<div class="index31_antikeimeno"></div></th>
            </tr>
        </thead>
        <tbody id="tbody_31">

            <%if(data.field_31_sxetiki_diataksi != null && data.field_31_sxetiki_diataksi.length != null ){
              for (var i = 0; i < data.field_31_sxetiki_diataksi.length; i++) { %>
            <tr id="table_31_row<%=i%>">
                <td>
                    <textarea class="form-control" id="field_31_sxetiki_diataksi<%=i%>"
                        name="field_31_sxetiki_diataksi<%=i%>" placeholder="Σχετική διάταξη αξιολογούμενης ρύθμισης"
                        rows="2"><%= Object.values(data.field_31_sxetiki_diataksi[i]) %></textarea>
                </td>
                <td>
                    <textarea class="form-control" id="field_31_synarmodia_ypoyrgeia<%=i%>"
                        name="field_31_synarmodia_ypoyrgeia<%=i%>"
                        placeholder="Συναρμόδια Υπουργεία – Συναρμόδιες υπηρεσίες / φορείς"
                        rows="2"><%= Object.values(data.field_31_synarmodia_ypoyrgeia[i]) %></textarea>
                </td>
                <td>
                    <textarea class="form-control" id="field_31_antikeimeno_synarmodiotitas<%=i%>"
                        name="field_31_antikeimeno_synarmodiotitas<%=i%>" placeholder="Αντικείμενο συναρμοδιότητας"
                        rows="2"><%= Object.values(data.field_31_antikeimeno_synarmodiotitas[i]) %></textarea>
                    <button class="btn remove float-right" type="button"><img src="/img/delete.png"
                            width="20px"></button>
                </td>
            </tr>
            <% }
              } %>

        </tbody>
    </table>

    <input type="button" id="add_row_table_31" name="add_row_table_31" class="float-right" value="Επιπλέον σειρά" />
    <br><br><br>

    <input type="button" name="previous" class="previous action-button-previous" value="Προηγούμενη φόρμα" />
    <input type="button" name="next" class="next action-button" value="Επόμενα πεδία" />
</fieldset>

