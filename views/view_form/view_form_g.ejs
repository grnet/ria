                    for (var i=0; i < data.field_30_diatakseis_katargisi.length; i++) { %>
                    <tr id="table_30_row<%=i%>">
                        <td> <textarea class="govgr-textarea " id="field_30_diatakseis_katargisi<%=i%>"
