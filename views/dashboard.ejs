<%- include('partials/head.ejs') %>
<script src="https://code.jquery.com/jquery-3.2.1.min.js"></script>
<link rel="stylesheet" href="//cdn.datatables.net/1.10.21/css/jquery.dataTables.min.css">
<script src="//cdn.datatables.net/1.10.21/js/jquery.dataTables.min.js"></script>

    <div class="MuiContainer-root jss110 MuiContainer-maxWidthLg">
        <div class="MuiGrid-root MuiGrid-container MuiGrid-spacing-xs-4">
            <!-- <%- include('sintaktis_form.ejs') %> -->
            <div class="MuiGrid-root MuiGrid-item MuiGrid-grid-xs-12 MuiGrid-grid-md-8">      
              <br><br>  
              <table id="data" class="display">
                <thead>
                    <tr>
                        <th>ID</th>
                        <th>Τίτλος</th>
                    </tr>
                </thead>
                <tbody>
                    
                    <% for(var i=0; i<entries.length; i++) {%>
                    <tr onclick="window.location.href='/form_a/<%= entries[i].id %>'">
                      <td><%= entries[i].id %></td>
                      <td><%= entries[i].title %></td>
                    </tr>
                    <% } %>
                    
                </tbody>
            </table>
            </div>
            <div class="MuiGrid-root MuiGrid-item MuiGrid-grid-xs-12 MuiGrid-grid-md-4">
              <div class="jss111">
                  <div class="jss274">
                      <h3 class="jss273 jss178 jss181">Χρήσιμοι Σύνδεσμοι</h3>
                      <ul class="jss275" id="progressbar">
                          <li class="active"><a href="/create">Νέα Αιτιολογική έκθεση</a></li>
                      </ul>
          
                  </div>
              </div>
          </div>
      </div>
    </div>
    <%- include('partials/footer.ejs') %>
  </div>
  <script>
    $(document).ready( function () {
        $('#data').DataTable();
    } );
  </script>
</body>
</html>