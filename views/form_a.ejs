<%- include('partials/head.ejs') %>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css"
        integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">

    <a href="/user_views/history" class="govgr-back-link" id="back" name="back"> <svg class="govgr-caret--left"
            viewBox="0 0 24 24" aria-hidden="true">
            <path d="M18,22V2L6,12L18,22z" />
        </svg> Επιστροφή στο ιστορικό</a>
    <nav class="govgr-horizontal-nav" id="nav-menu">
        <div class="govgr-horizontal-nav__container topnav" id="myTopnav">
            <div class="govgr-button-group">

                <input type="submit" id="final_save" class="govgr-btn govgr-btn-primary govgr-btn-cta create-new"
                    value="Οριστική Αποθήκευση" style="float: right;" />

                <input type="submit" id="save_temporarily" class="govgr-btn govgr-btn-secondary"
                    value="Προσωρινή αποθήκευση" style="float: right;" />

                <input type="button" name="delete" id="delete" class=" delete-analysis govgr-btn govgr-btn-warning "
                    style="float: right;" value="Διαγραφή" />

                <input type="button" name="pdfExport" class="govgr-btn govgr-btn-warning export-pdf"
                    value="Εξαγωγή σε PDF" />

                <input type="button" class="govgr-btn govgr-btn-secondary" style="float: right;" data-toggle="modal"
                    data-target="#versioningModal" id="modalButton" value="Ιστορικό Ανάλυσης" />


                <input type="button" name="convert" class="govgr-btn govgr-btn-warning convert"
                    style="display: none; float: right;" value="Μετατροπή" />
            </div>
    </nav>

    <div class="govgr-width-container">
        <div class="govgr-main-wrapper">
            <div class="govgr-grid-column-two-thirds">

                <form id="edit_form" enctype="multipart/form-data">

                    <div id="errors" style="display: none;">
                        <!--Display validation errors here-->
                        <!-- <div class="alert alert-danger">
                                <ul id="alerts"></ul>
                            </div> -->
                    </div>
                    <div>
                        <input type="hidden" id="status_ekthesis" name="status_ekthesis"
                            value="<%= data.status_ekthesis%>">
                        <input type="hidden" id="author" name="author" value="<%= data.author%>">
                    </div>
                    <fieldset id="arxiki" name="arxiki">

                        <h2 class="govgr-heading-l">Ανάλυση Συνεπειών Ρύθμισης</h2>
                        <div>
                            <h4 class="govgr-heading-m" id="subtitle"></h4>
                            <p class="govgr-body" id="subtext"></p>
                            <br />
                        </div>

                        <div class="modal fade" id="versioningModal" tabindex="-1" role="dialog"
                            aria-labelledby="versioningModalLabel" aria-hidden="true">
                            <div class="modal-dialog" role="document">
                                <div class="modal-content">
                                    <div class="modal-header">
                                        <h5 class="modal-title" id="versioningModalLabel">Ιστορικό Ανάλυσης</h5>
                                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                            <span aria-hidden="true">&times;</span>
                                        </button>
                                    </div>
                                    <div class="modal-body" id="modal_body">
                                        <div id="versions_table">
                                            <table id="versions"
                                                class="display govgr-table--zebra govgr-table--dense govgr-table--dark govgr-table versions-table">
                                                <thead class="govgr-table__head">
                                                    <tr class="govgr-table__row">
                                                        <th class="govgr-table__header" scope="col">Επιλογή</th>
                                                        <th class="govgr-table__header" scope="col">Ημερομηνία
                                                            αποθήκευσης</th>
                                                        <th class="govgr-table__header" scope="col">Ενημερώθηκε από</th>
                                                    </tr>
                                                </thead>
                                                <tbody class="govgr-table__body">

                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                    <div class="modal-footer">
                                        <button type="button" class="govgr-btn govgr-btn-warning action-button-delete"
                                            data-dismiss="modal">Κλείσιμο</button>
                                        <button type="button" id="modal_submit"
                                            class="govgr-btn govgr-btn-primary action-button">Υποβολή</button>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <br>
                        <div class="govgr-grid">
                            <div class="govgr-grid__col-span-6">
                                <label for="title" class="govgr-label-row required">Τίτλος αξιολογούμενης
                                    ρύθμισης</label>
                            </div>
                            <div class="govgr-grid__col-span-6">
                                <textarea class="govgr-textarea form-control" id="title" name="title"
                                    rows="3"><%= data.title%></textarea>

                                <!--<input type="text" class="govgr-input form-control" id="title" name="title"
                                    value="<%= data.title%>" />-->
                            </div>
                        </div>
                        <hr class="govgr-section-break govgr-section-break--m govgr-section-break--visible" />
                        <div class="govgr-grid">
                            <div class="govgr-grid__col-span-6">
                                <label for="typos_analysis" class="govgr-label-row required">Τύπος ανάλυσης</label>
                            </div>
                            <div class="govgr-grid__col-span-6">
                                <input type="text" class="govgr-input form-control" id="typos_analysis"
                                    name="typos_analysis" value="<%= data.typos_analysis%>" readonly />
                            </div>
                        </div>
                        <hr class="govgr-section-break govgr-section-break--m govgr-section-break--visible" />
                        <div class="govgr-grid">
                            <div class="govgr-grid__col-span-6">
                                <label for="initial_submit" class="govgr-label-row required">Αρχική
                                    καταχώρηση</label>
                            </div>
                            <div class="govgr-grid__col-span-6">
                                <input type="text" class="govgr-input form-control" id="initial_submit"
                                    name="initial_submit" value="<%= data.initial_submit %>" readonly />
                            </div>
                        </div>
                        <hr class="govgr-section-break govgr-section-break--m govgr-section-break--visible" />
                        <div class="govgr-grid">
                            <div class="govgr-grid__col-span-6">
                                <label for="initial_submit" class="govgr-label-row required">Τελευταία
                                    ενημέρωση</label>
                            </div>
                            <div class="govgr-grid__col-span-6">
                                <input type="text" class="govgr-input form-control" id="last_updated"
                                    name="last_updated" value="<%= data.last_updated %>" readonly>
                            </div>
                        </div>

                        <hr class="govgr-section-break govgr-section-break--m govgr-section-break--visible" />
                        <div class="govgr-grid">
                            <div class="govgr-grid__col-span-6">
                                <label for="epispeudon_foreas" class="govgr-label-row required">Επισπεύδων
                                    φορέας</label>
                            </div>
                            <div class="govgr-grid__col-span-6">
                                <textarea class="govgr-textarea" id="epispeudon_foreas" name="epispeudon_foreas"
                                    rows="3"><%= data.epispeudon_foreas %></textarea>
                            </div>
                        </div>

                        <hr class="govgr-section-break govgr-section-break--m govgr-section-break--visible" />
                        <div class="govgr-grid">
                            <div class="govgr-grid__col-span-6">
                                <label for="rythmisi_pou_afora" class="govgr-label-row">Ρύθμιση την οποία
                                    αφορά</label>
                            </div>
                            <div class="govgr-grid__col-span-6">
                                <textarea class="govgr-textarea" id="rythmisi_pou_afora" name="rythmisi_pou_afora"
                                    rows="3"><%= data.rythmisi_pou_afora %></textarea>
                            </div>
                        </div>
                        <hr class="govgr-section-break govgr-section-break--m govgr-section-break--visible" />
                        <div class="govgr-grid">
                            <div class="govgr-grid__col-span-6">
                                <label for="stoixeia_epikoinwnias" class="govgr-label-row">Στοιχεία
                                    επικοινωνίας</label>
                            </div>
                            <div class="govgr-grid__col-span-6">
                                <textarea class="govgr-textarea" id="stoixeia_epikoinwnias" name="stoixeia_epikoinwnias"
                                    rows="3"
                                    placeholder="E-mail, κινητό, γραφείο κ.ο.κ"><%= data.stoixeia_epikoinwnias %></textarea>
                            </div>
                        </div>
                        <hr class="govgr-section-break govgr-section-break--m govgr-section-break--visible" />
                        <div lass="govgr-grid">
                            <div class="govgr-grid__col-span-6">
                                <label class="govgr-label-row">Επισύναψη
                                    νομοσχεδίου</label> <br />
                            </div>
                            <div class="govgr-grid__col-span-6">
                                <input type="file" class="govgr-file-upload" id="nomosxedio" name="nomosxedio"
                                    accept=".pdf" />
                            </div>
                        </div>

                        <br>
                        <table id="nomosxedio_table" class="govgr-table--dark govgr-table">
                            <thead class="govgr-table__head">
                                <tr class="govgr-table__row">
                                    <th class="" colspan="2">Προηγούμενες αναρτήσεις:</th>
                                </tr>
                            </thead>
                            <tbody class="govgr-table__body">

                                <% if( data.nomosxedio && data.nomosxedio.length) { for (var i=data.nomosxedio.length-1;
                                    i>=0 ; i--) { %>
                                    <tr govgr-table__row>
                                        <td class="govgr-table__cell">
                                            <a href="/uploads/<%= data.nomosxedio[i].filename %> ">
                                                <%= data.nomosxedio[i].filename %>
                                            </a>
                                        </td>
                                        <td class="govgr-table__cell">
                                            <%= data.nomosxedio[i].upload_date %>
                                        </td>
                                    </tr>
                                    <% } }%>

                            </tbody>
                        </table>
                        <hr class="govgr-section-break govgr-section-break--m govgr-section-break--visible" />
                        <div>
                            <p class="govgr-body" id="tomeis_nomothetisis">Επιλέξατε από τον παρακάτω κατάλογο τον τομέα
                                ή τους τομείς νομοθέτησης
                                στους οποίους αφορούν οι
                                βασικές διατάξεις της αξιολογούμενης ρύθμισης: </p>
                            <div class="index_tomeis"></div>
                        </div>
                        <div class="govgr-checkboxes ">
                            <div class="govgr-checkboxes__item">
                                <label for="ekpedeusi_politismos" class="govgr-label govgr-checkboxes__label">ΕΚΠΑΙΔΕΥΣΗ
                                    - ΠΟΛΙΤΙΣΜΟΣ</label>
                                <input class="govgr-checkboxes__input" name="ekpedeusi_politismos" type="checkbox"
                                    id="ekpedeusi_politismos" />
                            </div>
                            <div class="govgr-checkboxes__item govgr-checkboxes--inline">
                                <label for="eksoteriki_politiki" class="govgr-label govgr-checkboxes__label">ΕΘΝΙΚΗ
                                    ΑΜΥΝΑ - ΕΞΩΤΕΡΙΚΗ
                                    ΠΟΛΙΤΙΚΗ
                                </label>
                                <input class="govgr-checkboxes__input" type="checkbox" id="eksoteriki_politiki"
                                    name="eksoteriki_politiki" value="eksoteriki_politiki" />
                            </div>
                            <div class="govgr-checkboxes__item">
                                <label for="forologiki_politiki" class="govgr-label govgr-checkboxes__label">ΟΙΚΟΝΟΜΙΚΗ
                                    / ΔΗΜΟΣΙΟΝΟΜΙΚΗ /
                                    ΦΟΡΟΛΟΓΙΚΗ ΠΟΛΙΤΙΚΗ
                                </label>
                                <input class="govgr-checkboxes__input" type="checkbox" id="forologiki_politiki"
                                    name="forologiki_politiki" value="forologiki_politiki" />
                            </div>
                            <div class="govgr-checkboxes__item">
                                <label for="koinoniki_politiki" class="govgr-label govgr-checkboxes__label">ΚΟΙΝΩΝΙΚΗ
                                    ΠΟΛΙΤΙΚΗ</label>
                                <input class="govgr-checkboxes__input" type="checkbox" id="koinoniki_politiki"
                                    name="koinoniki_politiki" value="koinoniki_politiki" />
                            </div>
                            <div class="govgr-checkboxes__item">
                                <label for="dimosia_dioikisi" class="govgr-label govgr-checkboxes__label">ΔΗΜΟΣΙΑ
                                    ΔΙΟΙΚΗΣΗ - ΔΗΜΟΣΙΑ ΤΑΞΗ -
                                    ΔΙΚΑΙΟΣΥΝΗ
                                </label>
                                <input class="govgr-checkboxes__input" type="checkbox" id="dimosia_dioikisi"
                                    name="dimosia_dioikisi" value="dimosia_dioikisi" />
                            </div>
                            <div class="govgr-checkboxes__item">
                                <label for="anaptiksi" class="govgr-label govgr-checkboxes__label">ΑΝΑΠΤΥΞΗ - ΕΠΕΝΔΥΤΙΚΗ
                                    ΔΡΑΣΤΗΡΙΟΤΗΤΑ</label>
                                <input class="govgr-checkboxes__input" type="checkbox" id="anaptiksi" name="anaptiksi"
                                    value="anaptiksi" />
                            </div>
                        </div> <br />
                        <div class="govgr-button-group">
                            <button type="button" class="govgr-btn govgr-btn-primary next action-button" name="next">
                                Επόμενα πεδία
                                <svg viewBox="0 0 24 24" class="govgr-svg-icon govgr-arrow--right" focusable="false"
                                    aria-hidden="true">
                                    <path d="M8.5,2L6.1,4.3l7.6,7.7l-7.6,7.6L8.5,22l10-10L8.5,2z" />
                                </svg>
                            </button>
                        </div>

                    </fieldset>

                    <fieldset id="tautotita_ruthmisis">
                        <h1 class="govgr-heading-l">Α. Αιτιολογική έκθεση</h1>

                        <h2>Η «ταυτότητα» της αξιολογούμενης ρύθμισης</h2>
                        <br /><br />

                        <div class="govgr-field">
                            <div class="govgr-label">
                                <label for="field_1" class="govgr-label-row required">1. Ποιο ζήτημα αντιμετωπίζει η
                                    αξιολογούμενη
                                    ρύθμιση;
                                </label>
                            </div>
                            <div class="govgr-grid__col-span-6">
                                <textarea class="govgr-textarea" id="field_1" name="field_1" rows="3"
                                    onkeypress="wordsCounter('field_1','words1', event)"
                                    onpaste="wordsCounter('field_1','words1', event)"><%= data.field_1 %></textarea>
                                <p style="float: right;">Λέξεις: <span id="words1"></span></p>
                            </div>
                        </div>
                        <hr class="govgr-section-break govgr-section-break--m govgr-section-break--visible" />
                        <div class="govgr-field">
                            <div class="govgr-label">
                                <label for="field_2" class="govgr-label-row required">2. Γιατί αποτελεί
                                    πρόβλημα;</label>
                            </div>
                            <div class="govgr-grid__col-span-6">
                                <textarea class="govgr-textarea" id="field_2" name="field_2" rows="3"
                                    onkeypress="wordsCounter('field_2','words2', event)"
                                    onpaste="wordsCounter('field_2','words2', event)"><%= data.field_2 %></textarea>
                                <p style="float: right;">Λέξεις: <span id="words2"></span></p>
                            </div>
                        </div>
                        <hr class="govgr-section-break govgr-section-break--m govgr-section-break--visible" />
                        <div class=govgr-field">
                            <div class="govgr-label">
                                <label for="field_3" class="govgr-label-row required">3. Ποιους φορείς ή πληθυσμιακές
                                    ομάδες αφορά;</label>
                            </div>
                            <div class="govgr-grid__col-span-6">
                                <textarea class="govgr-textarea" id="field_3" name="field_3" rows="3"
                                    onkeypress="wordsCounter('field_3','words3', event)"
                                    onpaste="wordsCounter('field_3','words3', event)"><%= data.field_3 %></textarea>
                                <p style="float: right;">Λέξεις: <span id="words3"></span></p>
                            </div>
                        </div>
                        <hr class="govgr-section-break govgr-section-break--m govgr-section-break--visible" />
                        <div class="govgr-button-group">
                            <button type="button" class="govgr-btn govgr-btn-primary previous action-button-previous"
                                name="previous">
                                <svg viewBox="0 0 24 24" class="govgr-svg-icon govgr-arrow--left" focusable="false"
                                    aria-hidden="true">
                                    <path d="M10.828 12l4.95 4.95-1.414 1.414L8 12l6.364-6.364 1.414 1.414z" />
                                    <!--  <path d="M8.5,2L6.1,4.3l7.6,7.7l-7.6,7.6L8.5,22l10-10L8.5,2z"/>-->
                                </svg> Προηγούμενα πεδία
                            </button>
                            <button type="button" class="govgr-btn govgr-btn-primary next action-button" name="next">
                                Επόμενα πεδία
                                <svg viewBox="0 0 24 24" class="govgr-svg-icon govgr-arrow--right" focusable="false"
                                    aria-hidden="true">
                                    <path d="M8.5,2L6.1,4.3l7.6,7.7l-7.6,7.6L8.5,22l10-10L8.5,2z" />
                                </svg>
                            </button>
                        </div>
                    </fieldset>

                    <fieldset id="anagkaiotita_rythmisis_1">
                        <h2 class="govgr-heading-m">Η αναγκαιότητα της αξιολογούμενης ρύθμισης (i)</h2> <br />
                        <div class="govgr-field">
                            <div class="govgr-label">
                                <label for="field_4" class="govgr-label-row required">4. Το εν λόγω ζήτημα έχει
                                    αντιμετωπιστεί με νομοθετική ρύθμιση στο παρελθόν;
                                </label>
                            </div>
                            <div class="govgr-grid__col-span-3">
                                <select id="field_4" name="field_4" class="govgr-select">
                                    <option selected disabled>Eπιλογή</option>
                                    <option value="Ναι">Ναι</option>
                                    <option value="Όχι">Όχι</option>
                                </select>
                            </div>
                        </div>
                        <div class="govgr-field" id="field_4_wrap" style="display:none;">
                            <div class="govgr-label">
                                <label for="field_4_1" class="govgr-label-row required">4.1 Ποιο είναι το ισχύον νομικό
                                    πλαίσιο που ρυθμίζει το
                                    ζήτημα;
                                </label>
                            </div>
                            <div class="govgr-grid__col-span-6">
                                <textarea class="govgr-textarea" id="field_4_1" name="field_4_comments" rows="3"
                                    onkeypress="wordsCounter('field_4_1','words4')"
                                    onpaste="wordsCounter('field_4_1','words4')"><%= data.field_4_comments %></textarea>
                                <p style="float: right;">Λέξεις: <span id="words4"></span></p>
                            </div>
                        </div>
                        <hr class="govgr-section-break govgr-section-break--m govgr-section-break--visible" /><br />
                        <div class="govgr-button-group">
                            <button type="button" class="govgr-btn govgr-btn-primary previous action-button-previous"
                                name="previous">
                                <svg viewBox="0 0 24 24" class="govgr-svg-icon govgr-arrow--left" focusable="false"
                                    aria-hidden="true">
                                    <path d="M10.828 12l4.95 4.95-1.414 1.414L8 12l6.364-6.364 1.414 1.414z" />
                                    <!--  <path d="M8.5,2L6.1,4.3l7.6,7.7l-7.6,7.6L8.5,22l10-10L8.5,2z"/> -->
                                </svg> Προηγούμενα πεδία
                            </button>
                            <button type="button" class="govgr-btn govgr-btn-primary next action-button" name="next">
                                Επόμενα πεδία
                                <svg viewBox="0 0 24 24" class="govgr-svg-icon govgr-arrow--right" focusable="false"
                                    aria-hidden="true">
                                    <path d="M8.5,2L6.1,4.3l7.6,7.7l-7.6,7.6L8.5,22l10-10L8.5,2z" />
                                </svg>
                            </button>
                        </div>
                    </fieldset>

                    <fieldset id="anagkaiotita_rythmisis_2">
                        <h2 class="govgr-heading-m">Η αναγκαιότητα της αξιολογούμενης ρύθμισης (ii)</h2>
                        <label class="govgr-label">5. Γιατί δεν είναι δυνατό να αντιμετωπιστεί στο πλαίσιο της
                            υφιστάμενης
                            νομοθεσίας:</label>
                        <br />
                        <div class="index_field_5"></div> <br />
                        <div class="govgr-field">
                            <div class="govgr-label">
                                <label for="field_5_1" class="govgr-label-row required">5.1 με αλλαγή προεδρικού
                                    διατάγματος, υπουργικής απόφασης ή
                                    άλλης κανονιστικής πράξης;
                                </label>
                            </div>
                            <div class="govgr-grid__col-span-6">
                                <textarea class="govgr-textarea" id="field_5_1" name="field_5_1" rows="3"
                                    onkeypress="wordsCounter('field_5_1','words5_1', event)"
                                    onpaste="wordsCounter('field_5_1','words5_1', event)"><%= data.field_5_1 %></textarea>
                                <p style="float: right;">Λέξεις: <span id="words5_1"></span></p>
                            </div>
                        </div>
                        <hr class="govgr-section-break govgr-section-break--m govgr-section-break--visible" />
                        <div class="govgr-field">
                            <div class="govgr-label">
                                <label for="field_5_2" class="govgr-label-row required">5.2 με αλλαγή διοικητικής
                                    πρακτικής συμπεριλαμβανομένης της
                                    δυνατότητας νέας ερμηνευτικής προσέγγισης της υφιστάμενης νομοθεσίας;
                                </label>
                            </div>
                            <div class="govgr-grid__col-span-6">
                                <textarea class="govgr-textarea" id="field_5_2" name="field_5_2" rows="3"
                                    onkeypress="wordsCounter('field_5_2','words5_2', event)"
                                    onpaste="wordsCounter('field_5_2','words5_2', event)"><%= data.field_5_2 %></textarea>
                                <p style="float: right;">Λέξεις: <span id="words5_2"></span></p>
                            </div>
                        </div>
                        <hr class="govgr-section-break govgr-section-break--m govgr-section-break--visible" />
                        <div class="govgr-field">
                            <div class="govgr-label">
                                <label for="field_5_3" class="govgr-label-row required">5.3 με διάθεση περισσότερων
                                    ανθρώπινων και υλικών
