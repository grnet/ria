            //TODO: refactor
            if (role === "Νομοπαρασκευαστική Επιτροπή (ΓΓΝΚΘ)" && $('#status_ekthesis').val() === "Οριστικοποιήθηκε" && $("#signed_pdf tbody>tr").length > 0) {
